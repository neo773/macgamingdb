{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/emails",
    "packages/server"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "prisma:db-push-prod": "bunx --env-file=.env.prod prisma db push",
    "prisma:db-push-local": "bunx prisma db push",
    "populate:aggregated-performance": "bun --bun scripts/populate-aggregated-performance.ts",
    "populate:review-count": "bun --bun scripts/add-review-count.ts",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "format": "bunx prettier --write ./src ./scripts",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.826.0",
    "@aws-sdk/s3-request-presigner": "^3.826.0",
    "@macgamingdb/emails": "workspace:*",
    "@macgamingdb/server": "workspace:*",
    "@prisma/adapter-libsql": "^7",
    "@prisma/client": "^7",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/components": "^0.0.36",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/date-fns": "^2.6.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "better-auth": "^1.2.5",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unicorn": "^61.0.2",
    "eslint-plugin-unused-imports": "^4.2.0",
    "linkedom": "^0.18.9",
    "lucide-react": "^0.532.0",
    "motion": "^12.23.11",
    "next": "^16.1.1",
    "next-themes": "^0.4.6",
    "prisma-query-formatter": "^1.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resend": "^4.2.0",
    "sonner": "^2.0.3",
    "superjson": "^2.2.2",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "ua-parser-js": "^2.0.3",
    "use-debounce": "^10.0.5",
    "vaul": "^1.1.2",
    "zod": "^3.24.2"
