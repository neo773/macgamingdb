    "prisma": "^7.2.0",
    "tailwindcss": "^4.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^4.0.16"
  },
  "packageManager": "bun@1.2.14"
